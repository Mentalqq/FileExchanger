<!DOCTYPE html>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - приложение ASP.NET</title>
    @Styles.Render("~/Content/css", "~/Content/Modal.css", "~/Content/style.css", "~/Content/AccessMessages.css")
    @Scripts.Render("~/bundles/modernizr", "~/scripts/jquery-3.4.1.min.js", "~/Scripts/Custom/Modal.js", "~/Scripts/Custom/Url.js")
</head>
<body>
    <content>
        @RenderBody()
    </content>


    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
